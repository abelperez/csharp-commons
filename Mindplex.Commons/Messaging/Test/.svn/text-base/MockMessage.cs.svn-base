
#region Imports

using System;

#endregion

namespace InternetBrands.Core.Messaging.Test
{
    /// <summary>
    /// 
    /// </summary>
    /// 
    [Serializable]
    public class MockMessage
    {
        /// <summary>
        /// 
        /// </summary>
        /// 
        private string name;

        /// <summary>
        /// 
        /// </summary>
        /// 
        public MockMessage()
        {
            this.name = "Test Message";
        }

        /// <summary>
        /// 
        /// </summary>
        /// 
        /// <param name="name"></param>
        public MockMessage(string name)
        {
            Guard.CheckIsNullOrEmpty(name);
            this.name = name;
        }

        /// <summary>
        /// 
        /// </summary>
        /// 
        public string Name
        {
            get { return name; }
            set { name = value; }
        }
    }
}
